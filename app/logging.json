{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
      "format": "%(levelname)-5s [%(filename)s:%(lineno)d] [%(name)s:%(funcName)s] %(message)s"
    }
  },
  "handlers": {
    "stdout": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "stream": "ext://sys.stdout"
    },
    "file": {
      "class": "logging.FileHandler",
      "formatter": "simple",
      "filename": "main.log",
      "mode": "w"
    }
  },
  "loggers": {
    "delegator.Delegator": {
      "level": "DEBUG"
    },
    "delegatee.DelegateeImpl1": {
      "level": "DEBUG"
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": [
      "stdout",
      "file"
    ]
  }
}